<%= render "secondary_menu" %>
<% cache [action_name, I18n.locale, parent.site, parent] do %>
	<%= render_collection_header %>
	<% @parent_categories.each do |c| %>
		<% if c.image_bank_photos.any? %>
			<h3><%= c.title %></h3>
			<%= grid_tag(@image_bank.column_count) do %>
				<%= render c.image_bank_photos %>
			<% end %>
		<% end %>
	<% end %>
<% end %>
